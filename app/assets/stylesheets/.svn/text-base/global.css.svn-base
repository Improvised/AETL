/* CSS Document */
* {
	margin: 0;
	border: 0;
	padding: 0;
	font-family: helvetica, sans-serif;
	font-size: 14px;
}
html {
	background: #1D2E3F;
}
body {
	text-align: center;
}
body>#wrapper {
	width: 312px;	
}
#wrapper {
	margin: 0 auto;
	width: 350px;
	padding: 0px 0px;
	
}
#page_bg {
	min-height: 500px;
	background: #D6DADD;
}
#container {
	padding: 0 4px 8px 4px;
	width: 704px;
}
#header {
	padding: 69px 0 0 254px;
	width: 704px;
	height: 90px;
	background: url("css_img/header_bg.jpg") top left no-repeat;
	text-align: left;
}
#container>#header {
	width: 454px;
	height: 21px;
}
ul.navigation {
	list-style-type: none;
}
ul.navigation li {
	padding: 0 12px;
	color: #F5D199;
	font-weight: bold;
	display: inline;
}
ul.navigation li a:link,
ul.navigation li a:active,
ul.navigation li a:visited {
	padding: 0;
	color: #FFF;
	text-decoration: none;
	font-weight: bold;
}
ul.navigation li a:hover {
	color: #0B3F7F;
}
body>#page_bottom {
	width: 696px;
}
#page_bottom {
	margin: 0 auto;
	padding: 6px 27px 0 27px;
	width: 750px;
	height: 13px;
	background: url("css_img/page_btm.gif") top center no-repeat;
	text-align: left;
}
.breadcrumbs {
	font-size: 14px;
	text-decoration: none;
	color: #74858C;
}
#content_right>p.sitemap {
	margin: -3px 12px 0 0;
}
.sitemap {
	margin: 0 12px 0 0;
	font-size: 14px;
	text-decoration: none;
	color: #74858C;
	text-align: right;
}
a.small_link:link,
a.small_link:active,
a.small_link:visited {
	padding: -3px 0 0 0;
	font-size: 14px;
	text-decoration: none;
	color: #74858C;
}
a.small_link:hover {
	color: #0B3F7F;
}
address {
	float: left;
	font-style: normal;
	font-size: 14px;
	color: #74858C;
	display: inline;
	text-align: left;
}
#page_bottom_right {
	text-align: right;
}
.legal {
	font-size: 14px;
	text-decoration: none;
	color: #74858C;
	text-align: right;
	display: inline;
}
.clear {
	clear: left;
}