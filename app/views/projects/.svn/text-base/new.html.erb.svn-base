  <div class="content">
    <h2 class="title">New Project</h2>
    <div class="inner">
      <% form_for @project, :html => {:class => :form} do |f|%>
        <%= render :partial => "project_form_new", :locals => {:f => f, :is_readonly => false}%>
        <div class="actions-bar">
          <%= submit_tag "Create" %><br><br>
          <div class="clear"/>
        </div>
      <% end -%>
    </div>
  </div>

